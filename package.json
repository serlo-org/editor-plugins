{
  "name": "@serlo-org/editor-plugins-workspace",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "precommit": "lint-staged",
    "build": "lerna run build",
    "ci": "lerna run --parallel ci",
    "format": "prettier --write \"{{{packages,plugins}/**,.storybook/**,example/**}/*,*}.{js,jsx,ts,tsx,css,scss,json,md,markdown}\"",
    "start": "npm-run-all storybook",
    "storybook": "start-storybook -p 9009 -s ./.storybook/public",
    "prebuild-storybook": "yarn build",
    "build-storybook": "build-storybook -c .storybook -s ./.storybook/public -o dist",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@splish-me/editor-core": "^0.2.0",
    "@splish-me/editor-ui": "^0.2.0",
    "@splish-me/editor-core": "^0.2.0",
    "emotion": "^9.2.10",
    "font-awesome": "^4.7.0",
    "react-emotion": "^9.2.10",
    "taskbook": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@storybook/addon-links": "^3.0.0",
    "@storybook/addon-options": "^3.0.0",
    "@storybook/addons": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "@types/jest": "^23.0.0",
    "@types/storybook__react": "^4.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.0.0",
    "babel-loader": "^8.0.0",
    "babel-runtime": "^6.0.0",
    "cross-env": "^5.0.0",
    "husky": "^0.14.0",
    "jest": "^23.0.0",
    "lerna": "^3.0.0",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^1.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.0.0",
    "typescript": "^3.0.0",
    "webpack-require-http": "^0.4.0"
  },
  "private": true,
  "lint-staged": {
    "{{{packages,plugins}/**,.storybook/**,example/**}/*,*}.{js,jsx,ts,tsx,css,scss,json,md,markdown}": [
      "prettier --write",
      "git add"
    ]
  },
  "workspaces": [
    "config",
    "packages/*",
    "plugins/*"
  ]
}
